<% include ./partials/head %>

<div class="container">
  <div class="jumbotron">
    <h1>Welcom to <%=title%></h1>      
    <% if(currentUser) { %>
      <p>User: <%= currentUser.username%></p>
      <p>UserLever: <%= currentUser.userlevel%></p>
    <% } %>
  </div>
  <a class="btn btn-primary" href="/bugs/new" role="button">Enter New Bug</a>
  <a class="btn btn-primary" href="/bugs/edit" role="button">Update Existing Bug</a>
  <% if(currentUser && currentUser.userlevel == 3 ) { %>
    <a class="btn btn-primary" href="/dbmaintance" role="button">Database Maintance</a>  
  <% } %>
</div>

<% include ./partials/footer %>