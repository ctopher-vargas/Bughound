              <th>Username</th>
              <th>User Level</th>
              <th></th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% users.forEach(function(user) { %>
              <tr>
                <td><a href="/users/edit/<%=user.emp_id%>"><%=user.emp_id%></a></td>
                <td><%=user.name%></td>
                <td><%=user.username%></td>
                <td><%=user.userlevel%></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
