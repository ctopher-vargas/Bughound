<% include ../partials/head %>

    <% include ../partials/search %>

    <div class="container content_body">
        <table class="table table-striped table-bordered">
          <thead class="thead-dark">
            <tr>
              <th>ID</th>
              <th>Name</th>
              <th>Username</th>
              <th>User Level</th>
            </tr>
          </thead>
          <tbody>
            <% users.forEach(function(user) { %>
              <tr>
                <td><a href="/users/edit/<%=user.emp_id%>"><%=user.emp_id%></a></td>
                <td><%=user.name%></td>
                <td><%=user.username%></td>
                <td><%=user.userlevel%></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
        <a class="btn btn-danger" href="/dbmaintance" role="button">Cancel</a>
    </div>
   
<% include ../partials/footer %>


